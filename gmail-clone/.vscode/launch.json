
{

    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\backend\\server.js"
        },

        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Program",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}\\client\\index.js"
          }
    ]
}